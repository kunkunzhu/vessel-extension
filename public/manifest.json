{
  "manifest_version": 3,
  "name": "Vessel Chrome Extension",
  "version": "1.0.0",
  "description": "Expand your vocabulary by collecting new words as you browse. ",
  "action": {
    "default_popup": "index.html",
    "default_icon": "vessel_icon.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["content.js"]
    }
  ]
}
